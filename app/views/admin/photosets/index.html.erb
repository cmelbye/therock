<h1>Listing photosets</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photosets.each do |photoset| %>
  <tr>
    <td><%= photoset.name %></td>
    <td><%= photoset.location %></td>
    <td><%= link_to 'Show', [:admin, photoset] %></td>
    <td><%= link_to 'Edit', edit_admin_photoset_path(photoset) %></td>
    <td><%= link_to 'Destroy', [:admin, photoset], confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photoset', new_admin_photoset_path %>
