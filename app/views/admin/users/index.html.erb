<div class="page-header">
	<h1>People</h1>
</div>

<table class="table table-striped table-bordered">
	<thead>
		<tr>
			<th>Name</th>
			<th>Email</th>
		</tr>
	</thead>
	<tbody>
	<% @users.each do |user| %>
		<tr>
			<td><%= user.name %></td>
			<td><%= user.email %></td>
		</tr>
	<% end %>
	</tbody>
</table>
</table> 

